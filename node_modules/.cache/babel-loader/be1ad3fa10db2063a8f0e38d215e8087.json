ination = true;\n                this.showLoadMore = false;\n                this.limit = 30;\n            },\n            setPokemonUrl(url) {\n                this.pokemonUrl = url;\n                this.showDetail = true;\n            },\n            closeDetail() {\n                 this.pokemonUrl = '';\n                 this.showDetail = false;\n            }\n        }\n    }\n</script>"],"sourceRoot":""}]}