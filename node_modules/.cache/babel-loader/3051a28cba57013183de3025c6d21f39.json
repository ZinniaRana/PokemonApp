pB;AAFP,MAAA,KAAK,EAAC,IAEC;AADP,MAAA,MAAM,EAAC,IACA;AAAP,MAAA,GAAG,EAAC;AAAG,KAHZ,E,IAAA,E;;AAAA,M,UAAA,C,EAIA,mBAAA,CAA2B,IAA3B,EAA2B,IAA3B,EAA2B,gBAAA,CAApB,OAAO,CAAC,IAAY,CAA3B,EAAmB;AAAA;AAAnB,K,CAPJ,E;;AAAA,M,UAAA,C;GAQU,CARV,E;;AAAA,GASE,EAVN,C,EAaU,MAAA,CAAA,Y,kBADV,mBAAA,CAMS,QANT,EAMS;UAAA;AAJJ,IAAA,QAAQ,EAAE,KAAA,CAAA,SAAA,KAAS,CAIf;AAHL,IAAA,KAAK,EAAC,KAGD;AAFJ,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,QAAA,CAAA,QAAA,EAAF;AAAA,KAAA;AAED,GANT,EAIwB,aAJxB,EAMA;AAAA;AANA,IAMA,UANA,C,uCASU,MAAA,CAAA,c,kBADV,YAAA,CAKsC,qBALtC,EAKsC;UAAA;gBAHzB,KAAA,CAAA,IAGyB;;aAHzB,KAAA,CAAA,I,GAAI,M;MAGqB;AAFjC,IAAA,OAAO,EAAE,GAEwB;AADjC,gBAAU,EACuB;AAAjC,IAAA,UAAQ,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,QAAA,CAAA,YAAA,CAAa,MAAb,CAAF;AAAA,KAAA;AAAyB,GALtC,E,IAAA,E;;AAAA,I,cAAA,C","sourcesContent":["<template>\n    <div class=\"list\">\n        <article v-for=\"(pokemon, index) in pokemons\" \n                 :key=\"'poke'+index\"\n                 @click=\"setPokemonUrl(pokemon.url)\">\n            <img :src=\"imageUrl + pokemon.id + '.png'\"\n                 width=\"95\"\n                 height=\"96\" \n                 alt=\"\">\n            <h3>{{ pokemon.name }}</h3>\n        </article>\n    </div>\n\n    <button \n        v-if=\"showLoadMore\"\n        :disabled=\"loadCount===9\"\n        class=\"btn\"\n        @click=\"loadMore()\">\n       Load More\n    </button>\n\n    <pagination \n        v-if=\"showPagination\"\n        v-model=\"page\" \n        :records=\"150\" \n        :per-page=\"30\" \n        @paginate=\"loadPokemons($event)\"/>\n   \n</template>\n\n<script>\n    import Pagination from 'v-pagination-3';\n    import MyPagination from './MyPagination.vue';\n\n    export default {\n        props: [\n            'imageUrl',\n            'apiUrl',\n            'showPagination',\n            'showLoadMore'\n        ],\n        data() {\n            return {\n                pokemons: [],\n                page: 1,\n                loadCount: 0\n            }\n        },\n        options: {\n            template: MyPagination    \n        },  \n        components: {\n            Pagination\n        },\n        methods: {\n            fetchPokemons(offset=0, limit=50) {\n                const axios = require('axios');\n                axios\n                .get(`${this.apiUrl}?offset=${offset}&limit=${limit}`)\n                .then((response) => {\n                    if(this.showPagination){\n                        this.pokemons=[];\n                    }\n                    if(\"results\" in response.data)\n                    {\n                        response.data.results.forEach(pokemon => \n                        {\n                            pokemon.id = pokemon.url\n                                            .split('/')\n                                            .filter((part) => {\n                                                return !!part\n                                            })\n                                            .pop();\n\n                            this.pokemons.push(pokemon);\n                        });\n                    } else {\n                        console.log('results key not found');\n                    }\n                })\n                .catch((error) => {\n                    console.log(error);    \n                })\n            },\n            setPokemonUrl(url) {\n                this.$emit('setPokemonUrl', url);\n            },\n            loadPokemons($event) {\n                console.log($event);\n                this.fetchPokemons((this.page-1) * 30, 30);\n            },\n            loadMore() {\n                this.loadCount++;\n                this.fetchPokemons(this.loadCount * 15, 15) \n            }\n        },\n        mounted() {\n            this.currentUrl = this.apiUrl;\n            this.fetchPokemons();\n        },\n    }\n</script>\n\n<style lang=\"scss\" scoped>\n  .list {\n    display: grid;\n    grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));\n    grid-gap: 10px;\n    width: 100%;\n    max-width: 680px;\n    article {\n      height: 150px;\n      background-color: #efefef;\n      text-align: center;\n      text-transform: capitalize;\n      border-radius: 5px;\n      cursor: pointer;\n      box-shadow: 0 15px 30px rgba(0,0,0,.2),\n                  0 10px 10px rgba(0,0,0,.2);\n      h3 {\n        margin: 0;\n      }\n    }\n  }\n  #scroll-trigger {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    width: 100%;\n    height: 150px;\n    font-size: 2rem;\n    color: #efefef;\n  }\n  [v-cloak] {\n    display: none;\n  }\n</style>\n"],"sourceRoot":""}]}