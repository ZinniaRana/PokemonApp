             pokemonUrl: '',\n                showDetail: false,\n                showLoadMore: false,\n                showPagination: false,\n                limit: 50\n            }\n        },\n        components: {\n            PokemonList,\n            PokemonDetails\n        },\n        methods: {\n            activeLoadMoreOption() {\n                this.showLoadMore = true;\n                this.showPagination = false;\n                this.limit = 50;\n            },\n            activePaginationOption() {\n                this.showPagination = true;\n                this.showLoadMore = false;\n                this.limit = 30;\n            },\n            setPokemonUrl(url) {\n                this.pokemonUrl = url;\n                this.showDetail = true;\n            },\n            closeDetail() {\n                 this.pokemonUrl = '';\n                 this.showDetail = false;\n            }\n        }\n    }\n</script>"],"sourceRoot":""}]}