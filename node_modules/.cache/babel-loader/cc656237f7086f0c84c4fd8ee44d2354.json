OAAO,CAAC,GAAR,CAAY,KAAZ;AACH,KAdD;AAeJ;AA7BW,CAAf","sourcesContent":["<template>\n    <div class=\"list\">\n        <article v-for=\"(pokemon, index) in pokemons\" \n                 :key=\"'poke'+index\">\n            <img :src=\"imageUrl + pokemon.id + '.png'\"\n                 width=\"95\"\n                 height=\"96\" \n                 alt=\"\">\n            <h3>{{ pokemon.name }}</h3>\n        </article>\n    </div>\n</template>\n\n<script>\n\n    export default {\n        props: [\n            // 'imageUrl',\n            // 'apiUrl'\n        ],\n        data: () => {\n            return {\n                // apiUrl: 'https://pokeapi.co/api/v2/pokemon',\n                pokemons: [],\n                imageUrl: ''\n            }\n        },\n        mounted: () => {\n            const axios = require('axios');\n            axios\n            .get(this.apiUrl)\n            .then((response) => {\n                console.log('Response ', response);\n                response.data.results.forEach(pokemon => {\n                    pokemon.id = pokemon.url.split('/')\n                                    .filter((part) =>{\n                                        return !!part\n                                    }).pop();\n                    this.pokemons.push(pokemon);\n                });\n            })\n            .catch((error) => {\n                console.log(error);    \n            })\n        }\n    }\n</script>"],"sourceRoot":""}]}