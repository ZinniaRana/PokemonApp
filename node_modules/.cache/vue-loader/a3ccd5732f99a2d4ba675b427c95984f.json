             <img :src=\"imageUrl + pokemon.id + '.png'\" alt=\"\">\n            </div>\n            <div v-if=\"pokemon\" class=\"data\">\n                <h2>{{ pokemon.name }}</h2>\n                <div class=\"property\">\n                    <div class=\"left\">Height</div>\n                    <div class=\"right\">{{ pokemon.height }} cm</div>\n                </div>\n                <div class=\"property\">\n                    <div class=\"left\">Weight</div>\n                    <div class=\"right\">{{ pokemon.weight }} lbs</div>\n                </div>\n            </div>\n\n            <h2 v-else>Pokemon was not found</h2>\n            <button class=\"close-btn\" @click=\"closeDetail\">Close</button>\n        </div>\n    </div>\n</template>\n\n<script>\n    export default {\n        props: [\n            'pokemonUrl',\n            'imageUrl'\n        ],\n        data: () => {\n            return {\n                show: false,\n                pokemon: {} \n            }   \n        },\n        methods: {\n            async fetchData() {\n                const axios = require('axios');\n                try{\n                    const response = await axios.get(this.pokemonUrl);\n                    this.pokemon = response.data;\n                    this.show = true;\n                }\n                catch(e){\n                    console.log(e);\n                }\n            },\n            closeDetail() {\n                this.$emit('closeDetail');\n            }\n        },\n        mounted() {\n            this.fetchData();\n        }\n    }\n</script>"]}]}