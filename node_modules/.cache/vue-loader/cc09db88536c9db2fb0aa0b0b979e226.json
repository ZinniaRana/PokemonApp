  }   \n        },\n        methods: {\n            async fetchData() {\n                const axios = require('axios');\n                try{\n                    const response = await axios.get(this.pokemonUrl);\n                    this.pokemon = response.data;\n                    this.show = true;\n                }\n                catch(e){\n                    console.log(e);\n                }\n            },\n            closeDetail() {\n                this.$emit('closeDetail');\n            }\n        },\n        mounted() {\n            this.fetchData();\n        }\n    }\n</script>"]}]}