okemonList,\n            PokemonDetails\n        },\n        methods: {\n            activeLoadMoreOption() {\n                this.showLoadMore = true;\n                this.showPagination = false;\n            },\n            activePaginationOption() {\n                this.showPagination = true;\n                this.showLoadMore = false;\n            },\n            setPokemonUrl(url) {\n                this.pokemonUrl = url;\n                this.showDetail = true;\n            },\n            closeDetail() {\n                 this.pokemonUrl = '';\n                 this.showDetail = false;\n            }\n        }\n    }\n</script>"]}]}